<!DOCTYPE html>
<html lang="en">
  
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Image Uploading</title>
</head>
<body>
    <div>
<table>
  <tr>
    <td>Name</td>
    <td>Category</td>
    <td>Price</td>
    <td>Seller</td>
    <td>Unit</td>
    <td>Available Unit</td>
    <td>Image</td>
    <td>link</td>
  </tr>
 <% items.forEach(function(item, index) { %>
    <tr>
      <td><%=item.name%></td>
      <td><%=item.category.name%></td>
      <td><%=item.sellPrice%></td>
      <td><%=item.seller.fullName%></td>
      <td><%=item.unit.value%> Kg</td>
      <td><%=item.availableUnit%></td>
      <% (item.images).forEach(function(image) {
        if (image.isDisplayImage) {%>
        <td><img src = "<%=image.img%>" width=100% height=100%></img></td>
      <% } else {}}) %>
      <td>items/view/<%=encItemIds[index]%>/<%=cipherTextEmail%></td>
    </tr>
  <% }) %>
</table>
</div>
</body>
</html>